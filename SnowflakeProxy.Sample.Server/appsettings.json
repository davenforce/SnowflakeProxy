{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "SnowflakeProxy": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Snowflake": {
    "Account": "YOUR_ACCOUNT",
    "User": "YOUR_USER",
    "PrivateKey": "YOUR_PRIVATE_KEY_PATH_OR_CONTENT",
    "PrivateKeyPassword": "YOUR_PRIVATE_KEY_PASSWORD",
    "Warehouse": "YOUR_WAREHOUSE",
    "Database": "YOUR_DATABASE",
    "Schema": "YOUR_SCHEMA",
    "Role": "YOUR_ROLE",
    "Application": "SnowflakeReportingSample",
    "ConnectionTimeout": 60,
    "RetryTimeout": 300,
    "MaxPoolSize": 10,
    "MinPoolSize": 2,
    "CommandTimeout": 300
  }
}
